        "@electron-forge/maker-squirrel": "^6.0.0-beta.50",
        "@electron-forge/maker-zip": "^6.0.0-beta.50",
        "devtron": "^1.4.0",
        "electron": "^12.0.12",
        "electron-packager": "^15.5.1",
        "electron-rebuild": "^1.11.0",
        "electron-wix-msi": "^2.2.0"
