{
    "name": "sparkle-tracker",
    "version": "1.0.0",
    "description": "It's Time Tracker APP.",
    "main": "main.js",
    "scripts": {
        "start": "electron .",
        "rebuild": "electron-rebuild -f -w iohook"
    },
    "keywords": [
        "Electron",
        "quick",
        "start",
        "tutorial",
        "time tracker"
    ],
    "build": {
