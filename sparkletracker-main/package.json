        "dmg": {
            "icon": "img/logo.icns"
        },
        "directories": {
            "buildResources": "img"
        }
    },
    "email": "contact@sparkleinfotech.com",
    "url": "http://sparkleinfotech.com/",
    "license": "CC0-1.0",
    "dependencies": {
        "axios": "^0.21.1",
