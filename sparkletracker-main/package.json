        "easytimer": "^1.1.1",
        "easytimer.js": "^4.2.0",
        "electron-abi": "^1.1.0",
        "electron-squirrel-startup": "^1.0.0",
        "electron-store": "^5.2.0",
        "electron-updater": "^4.3.9",
        "electron-winstaller": "^4.0.0",
        "iohook": "^0.9.3",
        "jquery": "^3.5.1",
        "jquery-validation": "^1.19.2",
        "log-to-file": "^2.0.2",
        "moment": "^2.30.1",
        "node-abi": "^2.18.0",
        "node-cron": "^2.0.3",
        "request": "^2.88.2",
